<%- contentFor('title') %> Database Form
<%- contentFor('body') %>

<h1>Form</h1>

 

<!-- Get the link you want, in this case school link-->
<form method="post" action="/form">
    <label for="title">Title:</label>
    <input type="text" id="title" name="title">
            <br>
    <label for="full_text">Fulltext:</label>
    <input type="text" id="full_text" name="full_text">
            <br> 
    <label for="from">From:</label>
    <input type="text" id="from" name="from">
            <br>
    <label for="media url">Media Url:</label>
    <input type="url" id="media_Url" name="media_Url">
            <br>
    <!--Submit and then it will direct you to school link-->
    <input type="submit" value="Submit">
  </form> 

